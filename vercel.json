{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs/static/(.*)",
      "dest": "node_modules/swagger-ui-dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/src/server.js"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "includeFiles": [
        "prisma/schema.prisma",
        "node_modules/.prisma/**/*"
      ]
    }
  }
}
